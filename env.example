# AI
# LLM Provider Configuration
LLM_PROVIDER=qwen  # Options: openai, qwen, etc.
MODEL_NAME=qwen3-max    # Model name to use
LLM_API_KEY=your_api_key_here
LLM_API_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# Application Configuration
MAX_ITERATIONS=5     # Maximum number of iterations for the agent loop

# Agent Prompts
# Override default prompts here. Use \n for newlines.
# Leave empty to use default hardcoded prompts.
PERSONA=

# Crawler
# Keywords to search (comma separated) - NOW PASSED VIA CLI ARGUMENT -keywords
# KEYWORDS=编程副业,编程兼职

# Cookies (if LoginType is cookie)
COOKIES=your_cookies_here

# Browser settings
HEADLESS=true
USER_DATA_DIR=browser_data

# Data saving
SAVE_DATA_OPTION=json

# Crawling settings
ENABLE_GET_COMMENTS=true
CRAWLER_MAX_SLEEP_SEC=2
MAX_NOTES=10
