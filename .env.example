# AI
# LLM Provider Configuration
LLM_PROVIDER=openai  # Options: openai, qwen, etc.
MODEL_NAME=gpt-4o    # Model name to use
LLM_API_KEY=your_llm_api_key_here
LLM_API_BASE_URL=https://api.openai.com/v1

# Application Configuration
MAX_ITERATIONS=5     # Maximum number of iterations for the agent loop

# Agent Prompts
# Override default prompts here. Use \n for newlines.
# Leave empty to use default hardcoded prompts.
SYSTEM_PROMPT=

# Crawler
# Cookies (if LoginType is cookie)
COOKIES=

# Browser settings
HEADLESS=true
USER_DATA_DIR=browser_data

# Data saving
SAVE_DATA_OPTION=json

# Crawling settings
ENABLE_GET_COMMENTS=true
CRAWLER_MAX_SLEEP_SEC=2
